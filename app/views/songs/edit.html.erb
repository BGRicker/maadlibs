<%= @song.artist %> - <%= @song.title %>

<div class="post-box col-xs-10 col-xs-offset-1">
  <%= simple_form_for @song do |f| %>
    <%= f.input :nouns %>
    <%= f.input :adjectives %>
    <%= f.input :people %>
    <%= f.input :verbs %>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
</div>
